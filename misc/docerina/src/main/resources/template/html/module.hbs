{{#partial "content" }}
    {{#if module.version}}
    <div>Version : {{ module.version }}</div>
    {{/if}}
    {{{ module.description }}}

    {{#if module.records}}
        <section class="method-content" id="records">
            <div class="main-method-title here">
                <a class="url-link" href="#records">
                    <img class="url-link-img" src="{{ rootPath }}html-template-resources/images/link.svg">
                </a>
                <h2> Records </h2>
            </div>
            <table>
                {{#each module.records}}
                    <tr>
                        <td class="module-title truncate records" id="{{name}}" title="{{name}}">
                            <a class="records {{#if isDeprecated}}strike{{/if}}" href="records/{{name}}.html">{{name}}</a>
                        </td>
                        <td class="module-desc">
                            {{#if isDeprecated}}
                            <span class="stab">Deprecated</span>
                            {{/if}}
                            {{{ editDescription description }}}
                        </td>
                    </tr>
                {{/each}}
            </table>
        </section>
    {{/if}}

