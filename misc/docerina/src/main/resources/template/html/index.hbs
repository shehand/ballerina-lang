<!DOCTYPE html>
<html>

{{> head }}

<body class="cBallerina-io">
  {{> bioBody }}
  <div class="navi-wrapper">
    <div class="navi-wrapper-content">
      {{> logo }}
    </div>
  </div>
  <div class="content-wrapper pusher">
    {{> mobMenu }}
    <div class="main-wrapper">
      <div id="search-impl" class="search-bar"></div>
      <div id="main">
        <h1>
          <a class="breadcrumb-home" href="{{ rootPath }}index.html">
            <img class="breadcrumb-home-icon" src="{{ rootPath }}html-template-resources/images/home.svg">
          </a>
          Modules</h1>
        <div>
          <table class="ui table">
            {{#if project.modules}}
            <tbody>
              {{#each project.modules}}
              <tr>
                <td class="module-title modules"><a href="{{id}}/index.html">{{id}}</a></td>
                <td class="module-desc">{{{ summary }}}</td>
              </tr>
              {{/each}}
            </tbody>
            {{/if}}
          </table>
        </div>
      </div>
    </div>
  </div>
  </div>
  </div>
</body>

{{> footer }}
